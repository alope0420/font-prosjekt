<%- include('header') %>

<table class="table bordered">
    <tr>
        <% for (const column of columns) { %>
            <th><%= column %></th>
        <% } %>
    </tr>
    <% for (const row of rows) { %>
        <tr>
            <% for (const cell of row) { %>
                <td><%= cell %></td>
            <% } %>
        </tr>
    <% } %>
</table>

<%- include('footer') %>